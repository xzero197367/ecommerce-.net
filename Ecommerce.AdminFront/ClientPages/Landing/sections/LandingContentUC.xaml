<UserControl x:Class="Ecommerce.AdminFront.ClientPages.Landing.sections.LandingContentUC"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:sections="clr-namespace:Ecommerce.AdminFront.ClientPages.Landing.sections"
             Loaded="UserControl_Loaded"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <ScrollViewer Grid.Row="1" VerticalScrollBarVisibility="Auto">
            <StackPanel Orientation="Vertical" Margin="10 0">
                <sections:SliderSectionUC/>
                <sections:CategorySection x:Name="categorySection"/>
                <sections:ProductSection x:Name="productSection"/>
               
                 <sections:FooterUC/> 
            
            </StackPanel>
        </ScrollViewer>
    </Grid>
</UserControl>
